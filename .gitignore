weapon_detection_env/
venv/
.venv/
env/
ENV/

# Ignore Python compiled and bytecode files
# These are generated automatically and not needed in version control
*.pyc
__pycache__/
*.pyo
*.pyd

# Ignore Jupyter Notebook checkpoints
# Auto-generated by Jupyter, not needed in version control
.ipynb_checkpoints/

# Ignore PyTorch model weights
# Large model files can be excluded if you manage them separately or regenerate them
*.pth

# Ignore log files and CSV data files
# These can be large or environment-specific; 
*.log
*.csv

# Ignore large or temporary data directories
# Excludes directories like Baggages
Dataset/

# Ignore editor-specific files
.vscode/

# Ignore model files
*.h5

